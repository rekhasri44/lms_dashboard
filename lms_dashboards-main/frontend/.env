# Production Configuration - Educational Dashboard
REACT_APP_API_BASE_URL=https://dashboard-backend-qmy9.onrender.com
REACT_APP_VERSION=2.0.0
REACT_APP_NAME=EduAdmin Enterprise
REACT_APP_ENVIRONMENT=production

# API Configuration
REACT_APP_API_TIMEOUT=30000
REACT_APP_API_RETRY_ATTEMPTS=3

# Feature Flags
REACT_APP_ENABLE_RBAC=true
REACT_APP_ENABLE_ANALYTICS=true
REACT_APP_ENABLE_REPORTS=true
REACT_APP_ENABLE_NOTIFICATIONS=true
REACT_APP_ENABLE_MONITORING=true
REACT_APP_ENABLE_AUTO_LOGOUT=true

# UI Configuration
REACT_APP_DEFAULT_THEME=light
REACT_APP_ITEMS_PER_PAGE=50
REACT_APP_AUTO_REFRESH_INTERVAL=30000

# Security
REACT_APP_TOKEN_REFRESH_INTERVAL=540000
REACT_APP_SESSION_TIMEOUT=3600000